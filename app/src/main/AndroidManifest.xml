<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.daffatahta.go2workapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.UserRegister"
            android:theme="@style/AppTheme.slidr" />
        <activity
            android:name=".activity.CompanyRegister"
            android:theme="@style/AppTheme.slidr" />
        <activity
            android:name=".activity.CompanyProfile"
            android:theme="@style/AppTheme.slidr" />
        <activity
            android:name=".activity.UserProfile"
            android:theme="@style/AppTheme.slidr" />
        <activity
            android:name=".activity.Login"
            android:theme="@style/AppTheme.slidr" />
        <activity
            android:name=".activity.FormLowongan"
            android:theme="@style/AppTheme.slidr" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".activity.Register"
            android:theme="@style/AppTheme.slidr" />
        <activity android:name=".activity.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>