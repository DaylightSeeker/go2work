        android:layout_marginLeft="43dp"
        android:layout_marginEnd="27dp"
        android:layout_marginRight="27dp"
        android:fontFamily="@font/roboto_regular"
        android:text="Tanggal Lahir"
        android:textColor="@color/putih"
        app:layout_constraintBaseline_toBaselineOf="@+id/userViewTanggalLahir"
        app:layout_constraintEnd_toStartOf="@+id/userViewTanggalLahir"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/userViewPendidikan"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="55dp"
        android:layout_marginRight="55dp"
        android:fontFamily="@font/roboto_regular"
        android:text="-"
        android:textColor="@color/putih"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/userViewStatus" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="43dp"
        android:layout_marginLeft="43dp"
        android:layout_marginEnd="49dp"
        android:layout_marginRight="49dp"
        android:fontFamily="@font/roboto_regular"
        android:text="Username"
        android:textColor="@color/putih"
        app:layout_constraintBaseline_toBaselineOf="@+id/userViewUsername"
        app:layout_constraintEnd_toStartOf="@+id/userViewUsername"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="43dp"
        android:layout_marginLeft="43dp"
        android:layout_marginEnd="76dp"
        android:layout_marginRight="76dp"
        android:fontFamily="@font/roboto_regular"
        android:text="Nama"
        android:textColor="@color/putih"
        app:layout_constraintEnd_toStartOf="@+id/userViewNama"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline13" />

    <TextView
        android:id="@+id/userViewStatus"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="55dp"
        android:layout_marginRight="55dp"
        android:fontFamily="@font/roboto_regular"
        android:text="-"
        android:textColor="@color/putih"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/userViewEmail" />

    <TextView
        android:id="@+id/userViewEmail"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="55dp"
        android:layout_marginRight="55dp"
        android:fontFamily="@font/roboto_regular"
        android:text="-"
        android:textColor="@color/putih"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/userViewUsername" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
